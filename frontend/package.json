    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^5.3.0"
  },
  "scripts": {
    "build": "DEPLOY_ENV=production webpack --mode=production",
    "build:staging": "DEPLOY_ENV=staging webpack --mode=production",
    "dev": "DEPLOY_ENV=development webpack-dev-server --mode=development",
    "lint": "yarn run lint:eslint && yarn run lint:stylelint",
    "lint:eslint": "eslint static --ext .ts --fix",
    "lint:stylelint": "stylelint 'static/styles/**/*.scss' --fix",
    "check:ts": "tsc --noEmit",
    "check:svelte": "svelte-check",
    "check:all": "yarn check:ts && yarn check:svelte && yarn lint",
    "openapi:gen": "openapi-generator-cli generate -g typescript-axios -i ../backend/schema.yml -o ./static/openapi"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "dayjs": "^1.9.4",
    "modern-css-reset": "^1.1.1",
    "skyway-js": "^3.1.0",
    "svelte": "^3.29.4"
  }
