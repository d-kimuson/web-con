from rest_framework.renderers import BaseRenderer, JSONRenderer
from typing import Any, Optional

class OpenApiYamlRenderer(BaseRenderer):
    media_type: str = ...
    format: str = ...
    def render(self, data: Any, media_type: Optional[Any] = ..., renderer_context: Optional[Any] = ...): ...

class OpenApiYamlRenderer2(OpenApiYamlRenderer):
    media_type: str = ...

class OpenApiJsonRenderer(JSONRenderer):
    media_type: str = ...
    def get_indent(self, accepted_media_type: Any, renderer_context: Any): ...

class OpenApiJsonRenderer2(OpenApiJsonRenderer):
    media_type: str = ...
